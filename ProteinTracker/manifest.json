{
  "manifest_version":2,

  "name":"Protein Tracer",
  "description":"Tracks a users proteins totals throuhout the day.",
  "version":"1.0",

  "browser_action":{
    "default_icon":"icon.png",
    "default_popup":"popup.html"
  },

  "options_page" : "options.html",

  "background":{
    "scripts":["eventPage.js"],
    "persistent": false
  },

  "permissions":[
    "storage",
    "notifications",
    "contextMenus"
  ]

}